{
      "name": "Exon Robot",
      "description": "Telegram Group Management Bot.",
      "keywords": [
         "telegram",
         "sexy",
         "management",
         "manager",
         "Exon"
      ],   
   "repository": "https://github.com/Abishnoi69/ExonRobot",
   "addons": [
      {
         "options": {
            "version": "12"
         }
      }
   ],
   "env": {
      "TOKEN": {
         "description": "Get bot token from @BotFather on TG",
         "required": true,
         "value": ""
      },
      "API_ID": {
         "description": "Get this value from my.telegram.org/apps.",
         "required": true,
         "value": ""
      },
      "API_HASH": {
         "description": "Get this value from my.telegram.org/apps.",
         "required": true,
         "value": ""
      },    
      "DATABASE_URI": {
         "description": "Your postgres sql db.",
         "required": true,
         "value": ""
      },      
      "OWNER_ID": {
         "description": "Your user ID as an integer.",
         "required": true,
         "value": ""
      },
      "OWNER_USERNAME": {
         "description": "Your username without the @",
         "value": ""
      },
      "SUPPORT_CHAT": {
         "description": "Your Telegram support group chat username where your users will go and bother you with shit But be like: MyGroupChatUsernameBlah. If this ever points to Exon Support than consider you made an enemy.",
         "required": true,
         "value": "AbishnoiMF"
      },
      "MONGO_DB_URI": {
         "description": "Required for database connections.",
         "required": true,
         "value": ""
      },
      "EVENT_LOGS": {
         "description": "Event logs channel to note down important bot level events, recommend to make this public. ex: '-123456'",
         "required": true,
         "value": ""
   }
   }
}
